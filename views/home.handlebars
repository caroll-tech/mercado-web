{{> partials/menu }}
<main>
<div class="container"> 
<h1 class="text-center p-4 mt-4 ">Bienvenido al Mercado Web, seleccione sus productos.</h1>
</div>
<div class="container"> 
<div class="row">
        {{#each productos}}
            <div class="col-md-4 px-5 mb-5">
                {{> partials/producto
                    nombre = this.name
                    imagen = this.imagen
                }}
            </div>
        {{/each}}
    </div>

</div>
{{> partials/modal }}
</main>